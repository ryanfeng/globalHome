        script.
            //全新版本，全新智能体验来回滚动,移动端不做处理
            if ($(window).width() >= 768) {
                $(".sm6-con1").addClass('active');
                var i = 1;
                var runData = funSetTime();
                $(".sm6-con").mouseenter(function () {
                    clearInterval(runData);
                    $(".sm6-con").removeClass('active');
                    $(".left-img").attr("src", "../images/produce/jf-phone" + $(this).attr('id').charAt(3) + ".png")
                }).mouseleave(function () {
                    i = $(this).attr('id').charAt(3);
                    $(".sm6-con" + i).addClass('active');
                    runData = funSetTime();
                })
                function funSetTime() {
                    runData = setInterval(function () {
                        $(".sm6-con").removeClass('active');
                        $(".sm6-con" + i).addClass('active');
                        $(".left-img").attr("src", "../images/produce/jf-phone" + i + ".png")
                        i++;
                        if (i == 4) {
                            i = 1;
                        }
                    }, 2000);
                    return runData;
                }
            } else {
                //移动端处理图片显示
                var width = $(window).width();
                $(".jf0-1").css('height', width * (776 / 768));
                $(".jf1-1").css('height', width * (778 / 768));
            }
            //头部下面导航页面的跳转
            (function () {
                var heightArr = [];
                for (var i = 1; i <= 5; i++) {
                    var id = "#moudle" + i;
                    var heightObj = {};
                    heightObj.id = i;
                    heightObj.top = $(id).offset().top;
                    heightArr.push(heightObj);
                }
                $(window).scroll(function () {
                    var topheight = $(window).scrollTop();
                    for (var j = heightArr.length - 1; j > -1; j--) {
                        if (topheight >= heightArr[j].top) {
                            $('.nav-class').removeClass('active');
                            $(".nav-class" + heightArr[j].id).addClass("active");
                            break;
                        }
                    }
                });
            })();


